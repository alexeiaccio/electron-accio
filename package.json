{
  "name": "electron-accio",
  "version": "0.1.0",
  "description": "Accio CMS",
  "repository": "https://github.com/alexeiaccio/electron-accio",
  "keywords": [
    "Electron"
  ],
  "author": "Alexei Accio <alexei.accio@gmail.com>",
  "license": "CC0-1.0",
  "scripts": {
    "postinstall": "ts-node -P scripts/tsconfig.json scripts/post-install.ts",
    "start": "cross-env NODE_ENV=development ts-node -P scripts/tsconfig.json scripts/start.ts",
    "start:prod": "cross-env NODE_ENV=production ts-node -P scripts/tsconfig.json scripts/start.ts",
    "compile:dev": "cross-env NODE_ENV=development parallel-webpack --config app/webpack.development.ts",
    "compile:prod": "cross-env NODE_ENV=production NODE_OPTIONS='--max_old_space_size=4096' parallel-webpack --config app/webpack.production.ts",
    "compile:script": "tsc -P scripts/tsconfig.json",
    "build:dev": "yarn compile:dev && cross-env NODE_ENV=development ts-node -P scripts/tsconfig.json scripts/build.ts"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^1.10.2",
    "@typescript-eslint/parser": "^1.10.2",
    "awesome-node-loader": "^1.1.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-minify-webpack-plugin": "^0.3.1",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.16.0",
    "event-kit": "^2.5.3",
    "express": "^4.17.1",
    "file-url": "^3.0.0",
    "front-matter": "^3.0.2",
    "html-webpack-plugin": "^3.2.0",
    "legal-eagle": "^0.16.0",
    "parallel-webpack": "^2.4.0",
    "prettier": "^1.18.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2",
    "webpack": "^4.35.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.1"
  },
  "devDependencies": {
    "@types/clean-webpack-plugin": "^0.1.3",
    "@types/electron-packager": "^13.0.0",
    "@types/event-kit": "^2.4.0",
    "@types/express": "^4.17.0",
    "@types/file-url": "^3.0.2",
    "@types/fs-extra": "^7.0.0",
    "@types/glob": "^7.1.1",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/legal-eagle": "^0.15.0",
    "@types/node": "^12.0.8",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@types/webpack": "^4.4.33",
    "@types/webpack-bundle-analyzer": "^2.13.1",
    "@types/webpack-dev-middleware": "^2.0.2",
    "@types/webpack-hot-middleware": "^2.16.5",
    "@types/webpack-merge": "^4.1.5",
    "electron": "^5.0.4",
    "electron-packager": "^14.0.0"
  }
}
